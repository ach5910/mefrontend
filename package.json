    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "4.3.0",
    "fs-extra": "^8.1.0",
    "fsevents": "2",
    "html-webpack-plugin": "4.0.0-beta.11",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "mini-css-extract-plugin": "0.9.0",
    "node": "^12.18.3",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "patch-package": "^6.2.2",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "postinstall-postinstall": "^2.1.0",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-spring": "^9.0.0-rc.3",
    "react-use-gesture": "^7.0.16",
    "resize-observer-polyfill": "^1.5.1",
    "resolve": "1.15.0",
    "resolve-url-loader": "3.1.1",
    "sass": "^1.26.10",
    "sass-loader": "8.0.2",
    "semver": "6.3.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "2.3.5",
    "ts-pnp": "1.1.6",
    "url-loader": "2.3.0",
    "webpack": "4.42.0",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "scripts": {
    "start": "PORT=8008 node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom-fourteen",
    "export": "bit export ach5910.mefrontend",
    "tag": "bit tag --all",
    "storybook": "start-storybook -p 6006",
    "storybook:debug": "start-storybook -p 6006 --debug-webpack",
    "storybook:docs": "start-storybook -p 6006 --docs",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bit": {
    "env": {
      "compiler": "ach5910.mefrontend/compilers/react@1.0.21",
      "tester": "ach5910.mefrontend/bit-tester@0.0.10"
    },
    "componentsDefaultDirectory": "src/{name}",
    "packageManager": "yarn",
    "overrides": {
      "test-utils": {
        "dependency": {
          "react-dom": "^16.13.1"
        },
        "devDependencies": {
          "@bit/ach5910.mefrontend.dom-testing-library": "0.0.1"
        }
      },
      "*": {
        "peerDependencies": {
          "react": "^16.13.1",
          "react-dom": "^16.13.1"
        },
        "devDependencies": {
          "@bit/ach5910.mefrontend.dom-testing-library": "0.0.1",
          "@bit/ach5910.mefrontend.react-testing-library": "0.0.2",
          "@bit/ach5910.mefrontend.user-event": "0.0.1",
          "@testing-library/dom": "^7.22.0",
          "@testing-library/jest-dom": "^5.11.0",
          "@testing-library/user-event": "^12.0.11"
        }
      },
      "bit-tester": {
        "dependency": {
          "babel-jest": "^24.9.0",
          "identity-obj-proxy": "3.0.0",
          "jest-environment-jsdom-fourteen": "1.0.1",
          "react-app-polyfill": "^1.0.6"
        },
        "peerDependencies": {
          "react": "^16.13.1",
          "react-dom": "^16.13.1"
        },
        "devDependencies": {
          "babel-jest": "^24.9.0",
          "camelcase": "^5.3.1",
          "identity-obj-proxy": "3.0.0",
          "jest-environment-jsdom-fourteen": "1.0.1",
          "react-app-polyfill": "^1.0.6"
        },
        "env": {
          "tester": "-"
        }
      }
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "react-is": "^16.13.1"
  },
  "resolutions": {
    "ast-types": "^0.14.0"
  }
}
